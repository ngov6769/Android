<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@drawable/back_mh"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:id="@+id/manhinh"

        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="90dp"
        android:layout_marginRight="30dp"
        android:background="@color/white"
        android:paddingStart="10dp"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/ketqua"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="139dp"
        android:layout_marginRight="30dp"
        android:background="@color/white"
        android:gravity="right"
        android:paddingEnd="10dp"
        android:text=""
        android:textSize="30dp" />

    <TextView
        android:id="@+id/chude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/aclonica"
        android:text="CASSIO"
        android:textSize="30dp" />
    <LinearLayout
        android:id="@+id/banphimtong"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/banphim1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/de"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:text="DE"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/c"
                android:text="C"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_marginRight="7dp"
                android:layout_marginLeft="7dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/ce"
                android:text="CE"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/amduong"
                android:text="+/-"
                android:layout_marginLeft="7dp"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/banphim2"
            android:layout_marginTop="7dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn7"
                android:text="7"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn8"
                android:text="8"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_marginRight="7dp"
                android:layout_marginLeft="7dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn9"
                android:text="9"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/cong"
                android:text="+"
                android:layout_marginLeft="7dp"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="7dp"
            android:id="@+id/banphim3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn4"
                android:text="4"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn5"
                android:text="5"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_marginRight="7dp"
                android:layout_marginLeft="7dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn6"
                android:text="6"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btntru"
                android:layout_marginLeft="7dp"
                android:text="-"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/banphim4"
            android:layout_marginTop="7dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn1"
                android:text="1"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn2"
                android:text="2"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_marginRight="7dp"
                android:layout_marginLeft="7dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn3"
                android:text="3"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/nhan"
                android:text="x"
                android:layout_marginLeft="7dp"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="7dp"
            android:id="@+id/banphim5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn0"
                android:text="0"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/phay"
                android:text=","
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_marginRight="7dp"
                android:layout_marginLeft="7dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bang"
                android:text=" = "
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/chia"
                android:text="/"
                android:layout_marginLeft="7dp"
                android:background="@drawable/background_btn"
                android:layout_width="70dp"
                android:layout_height="45dp"
                tools:ignore="MissingClass" />
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>